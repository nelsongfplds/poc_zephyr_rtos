&uart0 {
	compatible = "nordic,nrf-uarte";
};

&uart1 {
	compatible = "nordic,nrf-uarte";
	tx-pin = < 0x25 >;
	rx-pin = < 0x26 >;
};

&i2c1 {
	shtc3: shtc3@70 {
		compatible = "sensirion,shtcx";
		label = "SHTC3";
		reg = <0x70>;
		chip = "shtc3";
		measure-mode = "normal";
	};
};

/ {
	aliases {
		shtc3 = &shtc3;
	};
};

